{
  "name": "acter",
  "version": "0.0.0",
  "description": "The platform for coordinating action on global challenges",
  "main": "index.js",
  "repository": "git@github.com:acterglobal/code.git",
  "author": "Damon Toal-Rossi <damon@vestervand.com>",
  "license": "MIT",
  "private": true,
  "workspaces": [
    "packages/schema",
    "apps/*",
    "packages/*",
    "services/*"
  ],
  "scripts": {
    "dev": "yarn workspace @acter/web dev",
    "build": "yarn workspaces foreach run build",
    "start": "yarn workspace @acter/web start",
    "storybook": "yarn workspace @acter/components dev",
    "lint": "eslint . --ext .js,.mjs,.ts,.tsx",
    "test": "env-cmd jest",
    "test:prepush": "yarn lint --quiet && yarn cover && yarn build",
    "cover": "env-cmd --silent jest --coverage",
    "husky-install": "husky install",
    "clean:modules": "cli-confirm \"Are you you wan to clear all node_modules?\" && find . -name \"node_modules\" -type d -prune -exec rm -rf '{}' + && yarn && yarn generate",
    "clean:next": "rm -rf packages/web/.next",
    "clean:all": "yarn clean:next && yarn clean:all",
    "generate": "yarn workspace @acter/schema build",
    "db:migrate": "yarn workspace @acter/schema db:migrate",
    "db:migrate:run": "yarn workspace @acter/schema db:migrate:run",
    "db:migrate:prod": "yarn workspace @acter/schema db:migrate:prod",
    "db:reset": "yarn workspace @acter/schema db:reset",
    "db:seed": "yarn workspace @acter/schema db:seed"
  },
  "dependencies": {
    "@babel/core": "^7.14.8",
    "@babel/plugin-proposal-class-properties": "^7.12.1",
    "@babel/plugin-proposal-decorators": "^7.12.12",
    "@babel/plugin-proposal-private-property-in-object": "^7.14.5",
    "@babel/preset-env": "^7.14.9",
    "@babel/preset-react": "^7.14.5",
    "@babel/preset-typescript": "^7.14.5",
    "@material-ui/core": "^4.12.3",
    "axios": "^0.21.4",
    "babel-plugin-transform-typescript-metadata": "^0.3.1",
    "bullmq": "^1.40.1",
    "date-fns": "^2.22.1",
    "env-cmd": "^10.1.0",
    "graphql": "^15.3.0",
    "just-capitalize": "^1.0.1",
    "next-urql": "^3.2.1",
    "notistack": "^1.0.10",
    "pluralize": "^8.0.0",
    "pm2": "^5.1.0",
    "react": "^17.0.2",
    "react-cookie-consent": "^6.4.1",
    "react-dom": "^17.0.2",
    "react-is": "^17.0.2",
    "react-magnetic-di": "^2.2.6",
    "reflect-metadata": "^0.1.13",
    "tar": "^6.1.8",
    "urql": "^2.0.5"
  },
  "devDependencies": {
    "@babel/plugin-proposal-private-methods": "^7.14.5",
    "@mdx-js/react": "^1.6.22",
    "@storybook/addon-actions": "^6.3.4",
    "@storybook/addon-docs": "^6.3.4",
    "@storybook/addon-essentials": "^6.3.4",
    "@storybook/addon-links": "^6.3.4",
    "@storybook/builder-webpack5": "^6.3.4",
    "@storybook/manager-webpack5": "^6.3.4",
    "@storybook/react": "^6.3.4",
    "@testing-library/dom": "^7.29.4",
    "@testing-library/jest-dom": "^5.14.1",
    "@testing-library/react": "^12.0.0",
    "@testing-library/react-hooks": "^7.0.1",
    "@testing-library/user-event": "^13.2.1",
    "@trivago/prettier-plugin-sort-imports": "^2.0.4",
    "@types/graphql-fields": "^1.3.3",
    "@types/jest": "^26.0.24",
    "@types/markdown-draft-js": "^2.2.2",
    "@types/node": "^14.14.20",
    "@types/react": "^17.0.0",
    "@types/react-draft-wysiwyg": "^1.13.2",
    "@typescript-eslint/eslint-plugin": "^4.28.3",
    "@typescript-eslint/parser": "^4.28.3",
    "babel-jest": "^27.0.6",
    "babel-loader": "^8.2.2",
    "cli-confirm": "^1.0.1",
    "eslint": "^7.20.0",
    "eslint-plugin-jest": "^24.4.0",
    "eslint-plugin-you-dont-need-lodash-underscore": "^6.12.0",
    "graphql-tag": "^2.11.0",
    "husky": "^5.0.9",
    "identity-obj-proxy": "^3.0.0",
    "ignore-loader": "^0.1.2",
    "jest": "^27.0.6",
    "jest-transform-graphql": "^2.1.0",
    "netlify-cli": "^6.9.28",
    "syncpack": "^5.7.11",
    "ts-jest": "^27.0.4",
    "ts-node": "^10.0.0",
    "tsconfig-paths-webpack-plugin": "^3.3.0",
    "tslib": "^2.3.0",
    "typescript": "^4.3.5",
    "webpack": "^5.45.1"
  },
  "browser": {
    "fs": false,
    "os": false,
    "path": false
  },
  "engines": {
    "node": "16.x",
    "yarn": "3.x"
  },
  "cacheDirectories": [
    ".yarn/cache",
    "node_modules",
    "packages/components/node_modules",
    "packages/lib/node_modules",
    "packages/schema/node_modules",
    "packages/web/node_modules",
    "packages/web/.next/cache"
  ]
}
