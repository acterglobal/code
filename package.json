{
  "name": "acter",
  "version": "0.0.0",
  "description": "The platform for coordinating action on global challenges",
  "main": "index.js",
  "repository": "git@github.com:acterglobal/code.git",
  "author": "Damon Toal-Rossi <damon@vestervand.com>",
  "license": "MIT",
  "private": true,
  "workspaces": [
    "packages/schema",
    "packages/*"
  ],
  "scripts": {
    "dev": "yarn workspace @acter/web dev",
    "build": "yarn workspaces foreach run build",
    "start": "yarn workspace @acter/web start",
    "storybook": "yarn workspace @acter/components dev",
    "chromatic": "yarn workspace @acter/components chromatic",
    "lint": "eslint . --ext .js,.mjs,.ts,.tsx",
    "test": "env-cmd jest",
    "test:prepush": "yarn lint --quiet && yarn cover && yarn build",
    "cover": "env-cmd --silent jest --coverage",
    "husky-install": "husky install",
    "clean:modules": "cli-confirm \"Are you you wan to clear all node_modules?\" && find . -name \"node_modules\" -type d -prune -exec rm -rf '{}' + && yarn && yarn generate",
    "clean:next": "rm -rf packages/web/.next",
    "clean:all": "yarn clean:next && yarn clean:all",
    "generate": "yarn workspace @acter/schema build",
    "db:migrate": "yarn workspace @acter/schema db:migrate",
    "db:migrate:run": "yarn workspace @acter/schema db:migrate:run",
    "db:migrate:prod": "yarn workspace @acter/schema db:migrate:prod",
    "db:reset": "yarn workspace @acter/schema db:reset",
    "db:seed": "yarn workspace @acter/schema db:seed"
  },
  "dependencies": {
    "@material-ui/core": "4.12.4",
    "axios": "0.26.1",
    "bullmq": "1.78.2",
    "date-fns": "2.28.0",
    "env-cmd": "10.1.0",
    "graphql": "16.3.0",
    "just-capitalize": "3.0.1",
    "next-urql": "3.3.2",
    "node-fetch": "3.2.3",
    "notistack": "2.0.3",
    "pluralize": "8.0.0",
    "pm2": "5.2.0",
    "react": "18.0.0",
    "react-cookie-consent": "7.4.1",
    "react-dom": "18.0.0",
    "react-is": "18.0.0",
    "react-magnetic-di": "2.2.8",
    "reflect-metadata": "0.1.13",
    "tar": "6.1.11",
    "urql": "2.2.0"
  },
  "devDependencies": {
    "@mdx-js/react": "2.1.1",
    "@testing-library/dom": "8.13.0",
    "@testing-library/jest-dom": "5.16.4",
    "@testing-library/react": "13.0.0",
    "@testing-library/react-hooks": "7.0.2",
    "@testing-library/user-event": "14.0.4",
    "@trivago/prettier-plugin-sort-imports": "3.2.0",
    "@types/graphql-fields": "1.3.4",
    "@types/jest": "27.4.1",
    "@types/markdown-draft-js": "2.2.4",
    "@types/node": "16.11.26",
    "@types/react": "17.0.43",
    "@types/react-draft-wysiwyg": "1.13.4",
    "@typescript-eslint/eslint-plugin": "5.18.0",
    "@typescript-eslint/parser": "5.18.0",
    "cli-confirm": "1.0.1",
    "eslint": "8.12.0",
    "eslint-plugin-jest": "26.1.3",
    "eslint-plugin-you-dont-need-lodash-underscore": "6.12.0",
    "graphql-tag": "2.12.6",
    "husky": "7.0.4",
    "identity-obj-proxy": "3.0.0",
    "ignore-loader": "0.1.2",
    "jest": "27.5.1",
    "jest-transform-graphql": "2.1.0",
    "netlify-cli": "9.16.3",
    "shelljs": "0.8.5",
    "syncpack": "6.2.0",
    "ts-jest": "27.1.4",
    "ts-node": "10.7.0",
    "tsconfig-paths-webpack-plugin": "3.5.2",
    "tslib": "2.3.1",
    "typescript": "4.6.3",
    "webpack": "5.71.0"
  },
  "browser": {
    "fs": false,
    "os": false,
    "path": false
  },
  "engines": {
    "node": "16.x",
    "yarn": "3.x"
  },
  "cacheDirectories": [
    ".yarn/cache",
    "node_modules",
    "packages/components/node_modules",
    "packages/lib/node_modules",
    "packages/schema/node_modules",
    "packages/web/node_modules",
    "packages/web/.next/cache"
  ]
}
