{
  "name": "@acter/web",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "private": true,
  "scripts": {
    "dev": "yarn env next dev",
    "build": "yarn env next build",
    "build:cicd": "yarn workspace @acter/schema build && yarn build",
    "test": "yarn env:test -- yarn workspace @acter/schema prisma migrate reset --force",
