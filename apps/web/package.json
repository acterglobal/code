    "next-urql": "3.2.1",
    "notistack": "1.0.10",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-hotjar": "^5.0.0",
    "react-use-intercom": "1.4.0",
    "reflect-metadata": "0.1.13",
    "type-graphql": "1.1.1",
    "typescript": "4.6.4",
    "urql": "2.0.5"
