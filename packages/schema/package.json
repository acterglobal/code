{
  "name": "@acter/schema",
  "version": "1.0.0",
  "license": "MIT",
  "private": true,
  "scripts": {
    "build": "yarn generate && yarn build:schema && yarn build:codegen",
    "generate": "yarn prisma generate",
    "build:schema": "ts-node ./build-schema",
    "build:codegen": "graphql-codegen --config codegen.yml",
    "db:migrate": "yarn db:migrate:run --create-only",
    "db:migrate:run": "yarn env-cmd prisma migrate dev",
    "db:migrate:prod": "yarn env-cmd migrate deploy",
    "db:reset": "yarn env-cmd prisma migrate reset",
    "db:seed": "yarn env-cmd prisma db seed",
    "env-cmd": "env-cmd --silent --file ../../.env"
  },
  "dependencies": {
    "@auth0/nextjs-auth0": "1.7.0",
    "@prisma/client": "3.12.0",
    "bull": "4.8.1",
    "env-cmd": "10.1.0",
    "fp-ts": "2.11.9",
    "graphql-fields": "2.0.3",
    "prisma": "3.12.0",
    "type-graphql": "1.1.1",
    "typegraphql-prisma": "0.20.0",
    "uuid": "8.3.2"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "2.6.2",
    "@graphql-codegen/urql-introspection": "2.1.1",
    "@types/bull": "3.15.8",
    "@types/node": "16.11.26",
    "ts-node": "10.7.0",
    "typescript": "*"
  },
  "peerDependencies": {
    "date-fns": "*"
  },
  "prisma": {
    "seed": "ts-node prisma/seed.ts"
  }
}
